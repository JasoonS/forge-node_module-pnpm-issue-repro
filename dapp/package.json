{
  "name": "float.capital",
  "version": "1.0.0",
  "author": "Float Capital",
  "dependencies": {
    "@epnsproject/frontend-sdk": "^1.5.2",
    "@ramp-network/ramp-instant-sdk": "^2.5.0",
    "@ryyppy/rescript-promise": "^2.1.0",
    "@uauth/web3-react": "^0.7.7",
    "@web3-react/abstract-connector": "^6.0.7",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/torus-connector": "^6.2.9",
    "@web3-react/walletconnect-connector": "6.2.8",
    "@web3-react/walletlink-connector": "^6.2.11",
    "bs-css": "^15.3.0",
    "bs-css-emotion": "^2.5.1",
    "bs-fetch": "^0.6.2",
    "date-fns": "^2.17.0",
    "decco": "1.5.0",
    "ethereum-blockies-base64": "^1.0.2",
    "ethers": "^5.6.5",
    "graphql": "^15.5.0",
    "lodash": "^4.17.20",
    "next": "^12.2.4",
    "next-ga": "^2.3.4",
    "next-transpile-modules": "^9.0.0",
    "nprogress": "^0.2.0",
    "re-formality": "4.0.0-beta.16",
    "react": "^17.0.2",
    "react-confetti": "^6.1.0",
    "react-dom": "^17.0.2",
    "recharts": "2.1.12",
    "request": "^2.88.0",
    "rescript-future": "^1.1.0",
    "rescript-request": "^1.1.0",
    "styled-components": "^5.3.5",
    "swr": "^1.3.0"
  },
  "scripts": {
    "use-testnet-config": "cp src/config/separate-configs/ConfigTestnet.res src/config/ConfigSelected.res",
    "use-mainnet-config": "cp src/config/separate-configs/ConfigMainnet.res src/config/ConfigSelected.res",
    "use-graph-config": "cp src/config/separate-configs/ConfigGraph.res src/config/ConfigSelected.res",
    "next:dev": "NEXT_PUBLIC_DEVMODE=true next",
    "debug": "NODE_OPTIONS='--inspect' next",
    "now-build": "yarn res:build && next build",
    "next:export": "next export",
    "next:start": "next start -p $PORT",
    "check-config-selected": "[ -f \"src/config/ConfigSelected.res\" ] || (echo \"No ConfigSelected.res. Using ConfigMainnet.res\" && yarn use-mainnet-config)",
    "res:build": "yarn check-config-selected && rescript build -with-deps",
    "res:clean": "rescript clean -with-deps",
    "res:start": "yarn check-config-selected && rescript build -w -with-deps",
    "res:format": "rescript format -all",
    "dev": "npm-run-all --parallel res:start next:dev",
    "update-schema-local": "get-graphql-schema http://localhost:8000/subgraphs/name/float-capital/float-capital -j > graphql_schema.json",
    "update-schema": "get-graphql-schema https://api.thegraph.com/subgraphs/name/float-capital/float-capital -j > graphql_schema.json",
    "update-schema2": "get-graphql-schema https://test.graph.float.capital/subgraphs/name/float-capital/float-capital2 -j > graphql_schema.json",
    "update-dbschema-local": "get-graphql-schema http://localhost:8080/v1/graphql -h 'x-hasura-admin-secret=testing' -j > graphql_schema_db.json",
    "update-price-history-schema": "get-graphql-schema https://test.graph.float.capital/subgraphs/name/float-capital/float-oracle-prices -j > graphql_schema_price_history.json",
    "update-price-history-schema-dev": "get-graphql-schema https://api.thegraph.com/subgraphs/id/dev-graph-two -j > graphql_schema_price_history.json",
    "update-price-history-schema-local": "get-graphql-schema http://127.0.0.1:8000/subgraphs/name/float-capital/float-oracle-prices -j > graphql_schema_price_history.json",
    "update-univ2-schema": "get-graphql-schema https://api.thegraph.com/subgraphs/name/uniswap/uniswap-v2 -j > graphql_schema_univ2.json",
    "update-testnet-ui": "cd .. && node scripts/deployments.js > dapp/src/contractAddresses.json ; cd dapp && yarn update-schema ; yarn res:build",
    "visual-integration-test": "CYPRESS_UI_PORT=3000; start-server-and-test 'next start -p $CYPRESS_UI_PORT' http://localhost:3000 'cypress open'",
    "visual-integration-test-dev": "start-server-and-test 'dev' http://localhost:3000 'cypress open'",
    "integration-test-all": "CYPRESS_UI_PORT=3000; start-server-and-test 'next start -p $CYPRESS_UI_PORT' http://localhost:3000 'cypress run'",
    "integration-test-all-dev": "CYPRESS_UI_PORT=3000 start-server-and-test 'next start -p 3000' http://localhost:3000 'cypress open'",
    "integration-test": "cypress run",
    "integration-test-dev": "cypress open",
    "unit-test": "jest ./src",
    "update-schema-apmorgan-mumbai": "get-graphql-schema https://api.thegraph.com/subgraphs/name/float-capital/apmorgan-mumbai -j > graphql_schema_apmorgan.json",
    "update-schema-apmorgan-fuji": "get-graphql-schema https://api.thegraph.com/subgraphs/name/float-capital/apmorgan-fuji -j > graphql_schema_apmorgan.json"
  },
  "devDependencies": {
    "@apollo/client": "^3.3.7",
    "@ethersproject/experimental": "^5.1.2",
    "@float-capital/rescript-recharts": "^0.1.0",
    "@glennsl/bs-jest": "^0.7.0",
    "@reasonml-community/graphql-ppx": "^1.2.0",
    "@rescript/react": "^0.10.3",
    "@testing-library/cypress": "^7.0.6",
    "@truffle/hdwallet-provider": "^1.3.0",
    "autoprefixer": "^10.4.7",
    "bs-mocha": "^1.0.0",
    "cssnano": "^4.1.10",
    "cypress": "^9.2.1",
    "get-graphql-schema": "^2.1.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-cli": "^8.3.1",
    "rescript": "^9.1.2",
    "rescript-apollo-client": "^2.0.1",
    "start-server-and-test": "^1.12.1",
    "tailwindcss": "^3.0.24",
    "web3": "^1.3.5"
  }
}
