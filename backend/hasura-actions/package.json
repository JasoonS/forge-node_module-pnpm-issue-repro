  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "ts:build": "tsc",
    "res:build": "RES_LOG=* rescript build",
    "res:help": "rescript build -h",
    "res:start": "rescript build -w",
    "res:clean": "rescript clean",
    "start": "node ./src/index.js",
    "watch": "nodemon ./src/index.js",
    "event-manager": "nodemon ./src/events/EventManager.bs.js",
    "start-dev": "npm-run-all --parallel watch event-manager",
    "test": "nyc mocha ./src/Test.bs.js",
    "updateSchema": "get-graphql-schema http://localhost:8080/v1/graphql -h 'x-hasura-admin-secret=testing' -j > graphql_schema.json",
    "graphql-ppx-arm64-workaround": "node ./graphql-ppx-arm64-workaround.js"
  },
  "devDependencies": {
    "@reasonml-community/graphql-ppx": "^1.2.3",
    "@rescript/react": "^0.10.3",
    "@types/node": "^17.0.31",
    "dotenv": "^8.2.0",
    "get-graphql-schema": "^2.1.2",
    "mock-apollo-client": "^1.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "rescript": "^9.1.4",
    "rescript-apollo-client": "^3.1.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@apollo/client": "^3.4.12",
    "@ryyppy/rescript-promise": "^2.1.0",
    "@truffle/hdwallet-provider": "^1.7.0",
    "@yawaramin/prometo": "^0.11.1",
    "bn.js": "^5.1.3",
    "bs-fetch": "^0.6.2",
    "decco": "^1.5.0",
    "ethers": "^5.4.7",
    "express": "^4.17.1",
    "express-prom-bundle": "^6.4.1",
    "graphql-tag": "^2.11.0",
    "isomorphic-fetch": "^3.0.0",
    "kubemq-js": "^1.2.3",
    "prom-client": "^14.0.1",
    "rescript-express": "^0.4.1",
    "subscriptions-transport-ws": "^0.9.18",
    "truffle-privatekey-provider": "^1.2.0",
    "web3": "^1.3.1"
  }
